
class Point {
	field int x, y;

	function void Main()
	{
		var int x;
		var Point p;
		let p = Point.new();
		let x = p.getx();
		return;
	}

	constructor Point new(int ax, int ay)
	{
		let x = ax;
		let y = ay;
		return this;
	}

	method int getx() { return x; }
	method int gety() { return y; }

	method int add(Point other)
	{
		return ( (x + other.getx()) + (y + other.gety()) );
	}

	method void print()
	{
		do System.print();
		return getx();
	}

}
